<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Smart_parking"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Normal_slots"
            android:exported="false" />
        <activity
            android:name=".Parking_loc"
            android:exported="false" />
        <activity
            android:name=".Custom_notification"
            android:exported="false" />
        <activity
            android:name=".Custom_nearby"
            android:exported="false" />
        <activity
            android:name=".Payment"
            android:exported="false" />
        <activity
            android:name=".Notifications"
            android:exported="false" />
        <activity
            android:name=".Custom_slot"
            android:exported="false" />
        <activity
            android:name=".Book_slot"
            android:exported="false" />
        <activity
            android:name=".Complaints"
            android:exported="false" />
        <activity
            android:name=".View_slots"
            android:exported="false" />
        <activity
            android:name=".View_nearby_location"
            android:exported="false" />
        <activity
            android:name=".User_home"
            android:exported="false" />
        <activity
            android:name=".User_register"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".IpSetting"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".SocialDistanceAlert" />
        <service android:name=".LocationService" />
    </application>

</manifest>